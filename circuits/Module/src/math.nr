
//Add function 
pub fn add(x:Field ,y:Field) -> Field {
    x + y     
}

//Subtract function 
pub fn sub(x:Field,y:Field) -> Field{
    x - y 
}

//Multiplication function 
pub fn mul(x:Field, y:Field) -> Field {
    x * y 
}

//Division function 
pub fn div(x:Field, y:Field) -> Field {
    x / y 
}
